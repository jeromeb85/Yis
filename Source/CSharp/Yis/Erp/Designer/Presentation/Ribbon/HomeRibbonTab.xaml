<contract:RibbonTabBase x:Class="Yis.Erp.Designer.Presentation.Ribbon.HomeRibbonTab"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:ribbon="clr-namespace:System.Windows.Controls.Ribbon;assembly=System.Windows.Controls.Ribbon"
                        xmlns:contract="clr-namespace:Yis.Erp.Shell.Presentation.Contract"
                        mc:Ignorable="d"
                        d:DesignHeight="300"
                        d:DesignWidth="300"
                        Header="Designer">

    <ribbon:RibbonGroup x:Name="Form"
                        Header="Form">
        <ribbon:RibbonButton x:Name="FormDesigner"
                             Label="Form"
                             Click="FormDesigner_Click" />
    </ribbon:RibbonGroup>
</contract:RibbonTabBase>