<contract:RibbonTabBase x:Class="Yis.Erp.Mdm.Presentation.Windows.Ribbon.HomeRibbonTab"
                        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
                        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
                        xmlns:ribbon="clr-namespace:System.Windows.Controls.Ribbon;assembly=System.Windows.Controls.Ribbon"
                        xmlns:contract="clr-namespace:Yis.Erp.Shell.Presentation.Windows.Contract"
                        mc:Ignorable="d"
                        d:DesignHeight="300"
                        d:DesignWidth="300"
                        Header="Mdm">

    <ribbon:RibbonGroup x:Name="Client"
                        Header="Client">
        <ribbon:RibbonButton x:Name="NewFicheClient"
                             Label="Nouveau"
                             Click="NewFicheClient_Click" />
        <ribbon:RibbonButton x:Name="ListFicheClient"
                             Click="ListFicheClient_Click"
                             Label="Liste" />
    </ribbon:RibbonGroup>
</contract:RibbonTabBase>